{
    "env": "test",
    "branding": {
        "appName": "KőszegAPP",
        "supportEmail": "info@koszegapp.hu",
        "logoUrl": "https://koszegapp.hu/logo.png"
    },
    "qr": {
        "tokenLength": 32,
        "singleUse": true
    },
    "email": {
        "from": "KőszegTicket <onboarding@resend.dev>",
        "subjectPrefix": "KőszegTicket jegy:",
        "sendDelayMs": 500
    },
    "wallet": {
        "apple": {
            "enabled": true,
            "passNamePrefix": "",
            "teamIdentifier": "XXXXXXX",
            "passTypeIdentifier": "pass.hu.koszegapp.ticket"
        },
        "google": {
            "enabled": false,
            "issuerId": ""
        }
    },
    "pdf": {
        "enabled": true,
        "language": "hu"
    },
    "stripe": {
        "currency": "HUF",
        "successPath": "/tickets/success",
        "cancelPath": "/tickets"
    }
}